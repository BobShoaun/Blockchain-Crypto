{
  "name": "blockcrypto",
  "version": "4.0.2",
  "description": "Blockchain framework for making your own cryptocurrency in Node.js",
  "main": "dist/index.cjs",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "dependencies": {
    "bigint-conversion": "^2.1.12",
    "bip39": "^3.0.4",
    "bs58": "^4.0.1",
    "crypto-js": "^4.0.0",
    "elliptic": "^6.5.4",
    "hdkey": "^2.0.1"
  },
  "devDependencies": {
    "jest": "^26.6.3"
  },
  "jest": {
    "testEnvironment": "node"
